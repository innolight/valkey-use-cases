{
  "name": "valkey-use-cases",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Demonstrates different ways to use ValKey for various use cases",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "test": "pnpm -r test",
    "lint": "eslint . && pnpm -r lint",
    "lint:fix": "eslint . --fix && pnpm -r lint:fix",
    "format": "prettier --write . && pnpm -r format",
    "format:check": "prettier --check . && pnpm -r format:check",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "type-check": "tsc --build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f valkey"
  },
  "keywords": [
    "valkey",
    "nodejs",
    "typescript",
    "monorepo"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
